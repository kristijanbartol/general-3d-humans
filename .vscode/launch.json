                "--layers_camdsac", "700", "500", "300", "300",

                "--temp", "1.8",
                "--gumbel",
                "--entropy_beta_cam", ".01",
                //"--entropy_to_scores",
                "-lr", "0.0005",
                "-lrs", "10",
                "-ts", "5",
                "--temp_gamma", "0.9",

                "--train_iterations", "10",
                "--valid_iterations", "1",
                //"--test_iterations", "40",

                "--use_estimated",

                "--pose_hypotheses", "200",
                "--layers_posedsac", "1000", "900", "900", "900", "900", "700", 
                "--entropy_beta_pose", "0.01",
                "--est_beta", "0.02",
                "--exp_beta", "1.",
                "--body_lengths_mode", "2",
                "--pose_batch_size", "16",
                //"--weighted_selection"

                "--test",
            ]
        }
