    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--posedsac_only",
                //"--camdsac_only",

                "--dataset", "cmu-panoptic",
                "--cam_idxs", "1", "2", "3", "4", "6", "7", "10", "12", "16", "18", "19", "22", "23", "30",

                "--layers_camdsac", "700", "500", "300", "300",

                "--temp", "1.8",
                "--gumbel",
                "--entropy_beta_cam", ".01",
                //"--entropy_to_scores",
                "-lr", "0.0005",
                "-lrs", "10",
                "-ts", "5",
                "--temp_gamma", "0.9",

                "--train_iterations", "10",
                "--valid_iterations", "5",

                "--pose_hypotheses", "200",
                "--layers_posedsac", "1000", "900", "900", "900", "900", "700", 
                //"--layers_posedsac", "200", "300", "200",
                "--entropy_beta_pose", "0.01",
                "--weighted_beta", "0.02",
                "--body_lengths_mode", "1",
                "--pose_batch_size", "16",
                //"--weighted_selection"

                "--test",
                //"--filter_bad",
            ]
        }
    ]
}