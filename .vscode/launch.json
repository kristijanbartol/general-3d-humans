    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--posedsac_only",

                "--layers_camdsac", "200", "300", "200",

                "--temp", "1.8",
                "--gumbel",
                "--entropy_beta_cam", "1.",
                "--entropy_to_scores",
                "-lr", "0.001",
                "-lrs", "10",
                "-ts", "5",
                "--temp_gamma", "0.9",

                "--train_iterations", "10",
                "--valid_iterations", "0",

                "--pose_hypotheses", "100",
                "--layers_posedsac", "1000", "900", "900", "900", "900", "700", 
                //"--layers_posedsac", "200", "300", "200",
                "--entropy_beta_pose", "1.",
                "--body_lengths_mode", "1",
                "--pose_batch_size", "16",
                //"--weighted_selection"

                "--test",
                "--filter_bad",
            ]
        }
    ]
